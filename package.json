    "jquery": "^3.5.1",
    "particles.js": "^2.0.0",
    "reset-css": "^5.0.1",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^9.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "autoprefixer": "^9.8.4",
    "cssnano": "^4.1.10",
    "sass": "^1.26.7"
