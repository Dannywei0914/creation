 }\n}\n\n.body {\n  width: 100%;\n  height: 100%;\n  position: relative;\n}\n\n.homepage {\n  width: 100%;\n  height: 100%;\n  position: relative;\n  @include flex-jc-c;\n\n  &-logo {\n    width: 120px;\n    height: 120px;\n    border-radius: 50%;border: 3px solid #440044;\n    position: absolute;\n    top: 200px\n  }\n\n  &-title {\n    color: #000;\n    position: absolute;\n    font-size: 50px;\n    top: 350px;\n  }\n\n  &-background {\n    width: 100%;\n  }\n\n  &-down {\n    width: 30px;\n    height: 30px;\n    border: 3px solid #fff;\n    position: absolute;\n    top: 650px;\n    border-radius: 50%;\n    cursor: pointer;\n    @include flex-c;\n\n    img {\n      width: 70%;\n    }\n\n    &:hover {\n      opacity: .5;\n      transition: .3s;\n    }\n  }\n}\n\n.footer {\n  width: calc(100% - 100px);\n  height: 300px;\n  background-color: #0089A7;\n\n  padding: 50px 50px;\n\n  &-title {\n    font-size: 30px;\n    color: #FFFFFB;\n  }\n\n  &-list {\n    display: block;\n    margin-top: 50px;\n    @include flex-al-c;\n  }\n\n  &-router {\n    width: 30px;\n    height: 30px;\n    padding: 0 30px;\n\n    &-icon1 {\n      width: 30px;\n      height: 30px;\n    }\n\n    &-txt {\n      font-size: 15px;\n      color: #fff;\n      @include flex-al-c;\n\n      img {\n        padding-right: 10px;\n      }\n    }\n\n  &-icon2 {\n      width: 30px;\n      height: 30px;\n      display: none;\n    }\n\n    &:hover {\n      a>.footer-router-icon1 {\n        display: none;\n      }\n\n      a>.footer-router-icon2 {\n        display: block;\n      }\n    }\n  }\n}","style/public-flex.scss":"@mixin flex {\n  display: flex;\n}\n\n@mixin flex-c {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n@mixin flex-al-c {\n  display: flex;\n  align-items: center;\n}\n\n@mixin flex-jc-c {\n  display: flex;\n  justify-content: center;\n}\n\n@mixin flex-jc-left {\n  display: flex;\n  align-items: center;\n  justify-content: flex-start;\n}\n\n@mixin flex-jc-right {\n  di